# Environment variables
# Generated from Zod schema - do not edit manually
# Run `bun run generate:env-example` to regenerate

# Build-time variables (required before Nuxt starts)

# Auth layer URI (build-time only, use github:DCC-BS/nuxt-layers/no-auth to disable auth)
AUTH_LAYER_URI=github:DCC-BS/nuxt-layers/azure-auth

# Logger layer URI (build-time only, e.g., github:DCC-BS/nuxt-layers/pino-logger)
LOGGER_LAYER_URI=github:DCC-BS/nuxt-layers/pino-logger

# Runtime variables (used during application execution)

# API base URL
API_URL=http://localhost:8000

# GitHub token for feedback feature
FEEDBACK_GITHUB_TOKEN=

# Auth secret for session encryption (generate with: openssl rand -base64 32)
NUXT_AUTH_SECRET=

# Azure AD client ID
NUXT_AZURE_AUTH_CLIENT_ID=

# Azure AD tenant ID
NUXT_AZURE_AUTH_TENANT_ID=

# Azure AD client secret
NUXT_AZURE_AUTH_AZURE_CLIENT_SECRET=

# Azure AD API client ID
NUXT_AZURE_AUTH_AZURE_API_CLIENT_ID=

# Origin URL for Azure AD authentication
NUXT_AZURE_AUTH_ORIGIN=

# Logger level (default: debug)
LOG_LEVEL=debug

# Use dummy data (default: false)
DUMMY=false
